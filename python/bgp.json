{
  "ibgp" : {
    "required_config" : [
      "config_terminal"
    ],
    "variables" : [
      "as_number",
      "neighbor_ip",
      "neighbor_as"
    ],
    "command" : [
      "router bgp {as_number}",
      "neighbor {neighbor_ip} remote-as {neighbor_as}",
      "neighbor {neighbor_ip} send-community",
