{
    "enable" : {
        "command" : [
            "enable"
        ]
    },
    "exit" : {
        "command" : [
            "exit"
        ]
    },
    "config_terminal" : {
        "required_config" : [
            "enable"  
        ],
        "command" : [
            "configure terminal"
        ]
    },
    "config_interface" : {
        "required_config" : [
            "config_terminal"
        ],
        "variables" : [
            "interface_name"
        ],
        "command" : [
            "interface {interface_name}"
        ]
    },
    "config_ospf" : {
        "required_config" : [
            "config_terminal"
        ],
        "variables" : [
            "ospf_process_id",
            "router_id"
        ],
        "command" : [
            "router ospf {ospf_process_id}",
            "router-id {router_id}",
            "end"
        ]
    },
    "ip_address" : {
        "required_config" : [
            "config_interface"
        ],
        "variables" : [
            "ip_address",
            "mask"
        ],
        "command" : [
            "ip address {ip_address} {mask}",
            "no shutdown",
            "end"
        ]
    },
    "ospf" : {
        "required_config" : [
            "config_ospf",
            "config_interface"
        ],
        "variables" : [
            "area_id"
        ],
        "command" : [
            "ip ospf {ospf_process_id} area {area_id}",
            "end"
        ]
    },
    "mpls_global" : {
        "required_config": [
            "config_terminal"
        ],
        "command": [
            "mpls ip",
            "mpls label protocol ldp",
            "mpls ldp autoconfig",
            "end"
        ]
    },
    "mpls" : {
        "required_config" : [
            "config_interface"
        ],
        "command": [
            "mpls ip",
            "end"
        ]
    },
    "bgp" : {
        "required_config" : [
            "config_interface"
        ],
        "command": [
            "router bgp {as_number}",
            "neighbor {neighbor_ip_address} remote-as {as_number}",
            "address-family ipv4 unicast",
            "neighbor {neighbor_ip_address} activate",
            "end"
        ]
    }
}
