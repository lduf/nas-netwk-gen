{
    "enable" : {
        "command" : [
            "enable"
        ]
    },
    "exit" : {
        "command" : [
            "exit"
        ]
    },
    "config_terminal" : {
        "required_config" : [
            "enable"  
        ],
        "command" : [
            "configure terminal"
        ]
    },
    "config_interface" : {
        "required_config" : [
            "config_terminal"
        ],
        "variables" : [
            "interface_name"
        ],
        "command" : [
            "interface {interface_name}"
        ]
    },
    "config_ospf" : {
        "required_config" : [
            "config_terminal"
        ],
        "variables" : [
            "ospf_process_id",
            "router_id"
        ],
        "command" : [
            "router ospf {ospf_process_id}",
            "router-id {router_id}",
            "end"
        ]
    },
    "ip_address" : {
        "required_config" : [
            "config_interface"
        ],
        "variables" : [
            "ip_address",
            "mask"
        ],
        "command" : [
            "ip address {ip_address} {mask}",
            "no shutdown",
            "end"
        ]
    },
    "ospf" : {
        "required_config" : [
            "config_ospf",
            "config_interface"
        ],
        "variables" : [
            "area_id"
        ],
        "command" : [
            "ip ospf {ospf_process_id} area {area_id}",
            "end"
        ]
    },
    "mpls_global" : {
        "required_config": [
            "config_terminal"
        ],
        "command": [
            "mpls ip",
            "mpls label protocol ldp",
            "mpls ldp autoconfig",
            "end"
        ]
    },
    "mpls" : {
        "required_config" : [
            "config_interface"
        ],
        "command": [
            "mpls ip",
            "end"
        ]
    },
    "bgp_mod" : {
        "required_config" : [
            "config_terminal"
        ],
        "variables" : [
            "as_number"
        ],
        "command" : [
            "router bgp {as_number}"
        ]
    },
    "bgp_en" : {
        "required_config" : [
            "bgp_mod"
        ],
        "variables" : [
            "router-id"
        ],
        "command" : [
            "bgp router-id {router_id}"
        ]
    },
    "bgp" : {
        "required_config" : [
            "bgp_en"
        ],
        "variables" : [
            "network",
            "neighbor_ip",
            "neighbor_as"
        ],
        "command" : [
            "bgp log-neighbor-changes",
            "neighbor {neighbor_ip} remote-as {neighbor_as}",
            "address-family ipv4 unicast",
            "neighbor {neighbor_ip} activate",
            "network {network} mask {mask}",
            "exit"
        ]
    },
    "ibgp" : {
        "required_config" : [
            "bgp"
        ],
        "command" : [
            "neighbor {neighbor_ip} update-source Loopback0"
        ]
    },
    "ebgp_provider" : {
        "required_config" : [
            "community_list",
            "route_map",
            "bgp"
        ],
        "command" : [
            "address-family ipv4 unicast",
            "neighbor {neighbor_ip} route-map FROM_PROVIDER in",
            "neighbor {neighbor_ip} route-map TO_PROVIDER out",
            "end"
        ]
    },
    "ebgp_client" : {
        "required_config" : [
            "community_list",
            "route_map",
            "bgp"
        ],
        "command" : [
            "address-family ipv4 unicast",
            "neighbor {neighbor_ip} route-map TO_CLIENT in",
            "end"
        ]
    },
    "community_list_client" : {
        "required_config" : [
            "config_terminal"
        ],
        "variables" : [
            "list_number",
            "permit_type"
        ],
        "command" : [
            "ip community-list {list_number} permit internet",
            "end"
        ]
    },
    "community_list_provider" : {
        "required_config" : [
            "config_terminal"
        ],
        "variables" : [
            "list_number",
            "permit_type"
        ],
        "command" : [
            "ip community-list {list_number} permit internet",
            "end"
        ]
    },
    "route_map" : {
        "required_config" : [
            "config_terminal"
        ],
        "variables" : [
            "routemap_name",
            "list_number"
        ],
        "command" : [
            "route-map {routemap_name} permit 10",
            "match community {list_number}"
        ]
    },
    "route_map_pref" : {
        "required_config" : [
            "config_terminal"
        ],
        "variables" : [
            "routemap_name",
            "list_number",
            "community_name",
            "local_pref"
        ],
        "command" : [
            "route-map {routemap_name} permit 10",
            "match community {list_number}",
            "set local-preference {local_pref}",
            "set community {community_name}"
        ]
    }
}
